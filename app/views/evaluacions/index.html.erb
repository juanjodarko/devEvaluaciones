<div class="container app-header">
<p id="notice"><%= notice %></p>

<h1>Listado de Evaluaciones</h1>

<table clasS="table table-striped">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluacions.each do |evaluacion| %>
      <tr>
        <td><%= evaluacion.nombre %></td>
        <td><%= evaluacion.tipo %></td>
        <td><%= link_to 'Show', evaluacion %></td>
        <td><%= link_to 'Edit', edit_evaluacion_path(evaluacion) %></td>
        <td><%= link_to 'Destroy', evaluacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Evaluacion', new_evaluacion_path, class: "btn btn-primary" %>
</div>
